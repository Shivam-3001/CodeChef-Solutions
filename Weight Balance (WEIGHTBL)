#include <iostream>
using namespace std;

int main() {
	// your code goes here
	int t;
	cin>>t;
	while(t--){
	    int w1, w2, x1, x2, m;
	    cin>>w1>>w2>>x1>>x2>>m;
	    int kg=w2-w1;
	    int first=x1*m;
	    int second=x2*m;
	    if(kg>=first && kg<=second) cout<<1<<endl;
	    else    cout<<0<<endl;
	}
	return 0;
}
