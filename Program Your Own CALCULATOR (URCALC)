#include <iostream>
using namespace std;

int main() {
	// your code goes here
	double a, d;
	cin>>a;
	double b;
	cin>>b;
	char ch;
	cin>>ch;
	switch (ch){
	    case '+':cout<<(a+b)<<endl;
	            break;
	            
	    case '-':cout<<(a-b)<<endl;
	            break;
	            
	    case '*':cout<<(a*b)<<endl;
	            break;
	    case '/':d=(double)a/b;
	             cout<<fixed<<d<<endl;
	             break;
	}
	return 0;
}
